using System;
namespace ConsoleApp7
{
    class Program
    {
        static void Main(string[] args)
        {
            Console.WriteLine("введите число от 1 до 9(включительно)");
            double a=Convert.ToDouble(Console.ReadLine());
            {
                if (a < 10)
                {
                    double i = 1;
                    for (i = 1; i < 10; i++)
                    {
                        Console.WriteLine(a + "*" + i + " =" + a * i);
                    }
                }
                else
                {
                    Console.WriteLine("вы ввели число больше или 9 или меньше 1,повторите попытку");
                }
                Console.ReadKey();
            }
        }
    }
}
